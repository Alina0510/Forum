@model IEnumerable<Forum.Models.User>

<!doctype html>
<html>
<head>
    <title>Forum</title>
    <meta charset="utf-8" />
</head>
<body>
    <p><a asp-action="Registration">Register</a></p>
    <h1>Hello to our Forum!</h1>
    <table class="table">
        <tr><th>Nickname</th><th>Email</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nickname</td>
            <td>@item.Email</td>
        </tr>
    }
</table>
</body>
</html>